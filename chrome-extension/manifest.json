{
  "manifest_version": 3,
  "name": "WeRead Cookie Exporter",
  "version": "1.0",
  "description": "Extracts WeRead cookies for API access",
  "permissions": ["cookies"],
  "host_permissions": ["https://weread.qq.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png"
    }
  },
  "icons": {
    "16": "images/icon16.png"
  }
}
